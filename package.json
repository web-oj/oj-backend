{
  "name": "web-backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "predev": "npm run swagger",
    "prebuild": "npm run swagger",
    "build:start": "tsc && tsoa spec && npm run start",
    "build": "tsc",
    "dev": "concurrently \"nodemon -r tsconfig-paths/register\" \"nodemon -x tsoa spec\"",
    "swagger": "tsoa spec"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "ts-node -r tsconfig-paths/register ./src/index.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.7.5",
    "@types/swagger-ui-express": "^4.1.6",
    "concurrently": "^9.0.1",
    "nodemon": "^3.1.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "keccak256": "^1.0.6",
    "morgan": "^1.10.0",
    "mysql2": "^3.11.3",
    "swagger-ui-express": "^5.0.1",
    "tsconfig-paths": "^4.2.0",
    "tsoa": "^6.5.0",
    "typeorm": "^0.3.20"
  }
}
